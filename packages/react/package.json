  },
  "dependencies": {
    "@library-token/core": "^0.0.1"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@types/react": "19.0.1",
    "react": "19.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
